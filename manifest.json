{
    "name": "snsr",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "src/background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest"
    ],
    "host_permissions": [
        "*://*.google.com/",
        "*://*.sol/*"
    ]
  }