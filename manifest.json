{
    "name": "Solana Name Service Resolver",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "description": "Use .sol domains in the browser!",
    "background": {
      "service_worker": "src/background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "webRequest"
    ],
    "host_permissions": [
        "*://www.google.com/search?q=*",
        "*://*.sol/*"
    ]
  }